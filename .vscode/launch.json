    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Swingland",
            "request": "launch",
            "classPaths": [ "${workspaceFolder}/bin/test.jar" ],
            "cwd": "${workspaceFolder}",
            "sourcePaths": [ "src" ],
            "stopOnEntry": true,
            "vmArgs": "-Djava.library.path=bin",
            "mainClass": "com.ashbysoft.test.Test",
            "args": [ "swingland" ],
            "env": {
                "SWINGLAND_SUBSURFACES": "True"
            }
        },
        {
            "type": "java",
            "name": "Fed",
            "request": "launch",
            "classPaths": [ "${workspaceFolder}/bin/fed.jar" ],
            "cwd": "${workspaceFolder}",
            "sourcePaths": [ "src" ],
            "stopOnEntry": true,
            "vmArgs": "-Djava.library.path=bin",
            "mainClass": "com.ashbysoft.fed.Fed",
            "args": [ "-f", "res/cursors/DEFAULT" ]
        },
        {
            "type": "java",
            "name": "PCFTest",
            "request": "launch",
            "classPaths": [ "${workspaceFolder}/bin/test.jar" ],
            "cwd": "${workspaceFolder}",
            "sourcePaths": [ "src" ],
            "stopOnEntry": true,
            "vmArgs": "-Djava.library.path=bin",
            "mainClass": "com.ashbysoft.test.PCFTest",
            "args": "X11/misc/jiskan24.pcf.gz"
        }

    ]
}